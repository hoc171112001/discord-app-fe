{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "baseUrl": "./src",
    "noEmit": true,
    "jsx": "preserve",
    "paths": {
      "@/*": ["./src/*"],
      "@components/*": ["./components/*"],
      "@assets/*": ["./assets/*"],
      "@constants/*": ["./constants/*"],
      "@pages/*": ["./pages/*"],
      "@styles/*": ["./styles/*"],
      "@config/*": ["./config/*"],
      "@redux/*": ["./redux/*"],
      "@utils/*": ["./utils/*"],
      "@shared/*": ["./shared/*"]
    }
  },
  "include": ["src"]
}
